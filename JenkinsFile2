pipeline {
   agent any
   tools {
        maven 'Maven'

    }
   stages {
   
        stage('Build') {
            steps {
               snDevOpsStep ()
                sh 'mvn -B -DskipTests clean compile'
            }
        }
        
		stage('Deploy') {

            steps {
                //snDevOpsChange()
				snDevOpsStep ()
                //error("Build failed because of this and that..")
                echo 'Executing deploy'

            }
      }
      
  }
}
