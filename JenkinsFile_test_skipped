pipeline {
    agent any
    stages {
        stage('Stage 1') {
            when {
                equals expected: 2, actual: 1
            }
            steps {
                snDevOpsChange()
                echo 'Executing stage 1'
                //sh 'fail'
                sleep 1
            }
        }
        stage('Stage 2') {
            when {
                equals expected: 2, actual: 1
            }
            steps {
                echo 'Executing stage 2'
                // sh 'fail'
                snDevOpsChange()
                sleep 1
            }
        }

        stage('Stage 3') {
            when {
                equals expected: 2, actual: 3
            }
            steps {
                snDevOpsChange()
                echo 'Executing stage 3'
                sleep 1
            }
        }
        stage('Stage 4') {
            when {
                equals expected: 2, actual: 3
                
            }
            steps {
                //snDevOpsChange()
 				//sh 'false'
                error("Stage 4: failed , testing pipeline UI")

                sleep 1
            }
        }

    }
}